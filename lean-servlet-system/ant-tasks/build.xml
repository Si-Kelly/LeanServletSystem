<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_jar" name="Create Lean Servlet Utils jar">
    <target name="create_jar">
        <jar destfile="../target/lean-servlet.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Built-By" value="${user.name}"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="../bin"/>
        </jar>
    </target>
</project>